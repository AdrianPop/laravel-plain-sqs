    "keywords": ["php","laravel","lumen","sqs","aws","xml"],
    "homepage": "https://github.com/keithbrink/laravel-plain-sqs",
    "license": "MIT",
    "authors": [
        {
            "name": "Keith Brink",
            "email": "keith.brink@gmail.com"
        }
    ],
